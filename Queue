//C++ code for Queue 

#include<iostream>
using namespace std;

class Queue
{
private:
int front;
   int rear;
int size;
int* Q;
public:
Queue(){front=rear=-1;size=10;Q=new int[size];}
   Queue(int size){front=rear=-1;this->size=size;Q=new int[this->size];}
void enqueue(int x);
int dequeue();
void display();
};


void Queue::enqueue(int x)
{
if(front==size-1){
cout<<"Queue Full\n";
}
else{
front++;
Q[front]=x;
}
}

void Queue::display(){
for(int i=rear+1;i<=front;i++){
cout<<Q[i]<<" ";

int main(){
   
Queue q;
q.enqueue(1);
q.enqueue(2);
q.enqueue(3);
q.enqueue(4);
q.enqueue(5);
q.enqueue(6);
q.enqueue(7);
q.enqueue(8);
q.enqueue(9);
q.enqueue(10);

    q.enqueue(11);
q.enqueue(12);

q.display();



return 0;

}
